package sd;

public class Directeur {

	private String nom;
	private String prenom;
	private int point;

	public Directeur(String prenom,String nom){
		this.prenom=prenom;
		this.nom=nom;
		this.point=0;
	}
	
	public String getPrenom(){
		return this.prenom;
	}
	public String getNom(){
		return this.nom;
	}
	
	public int calculPoint(ListeFilm Liste){
		for(int i=0;i<Liste.size();i++){			
			if(Liste.get(i).getDirecteur().getPrenom()==this.prenom && Liste.get(i).getDirecteur().getNom()==this.nom){
				System.out.println("ddddiiiirrrreeecccttt");
				this.point++;
				System.out.println(this.prenom+this.nom+":"+this.point);
			}
		}
		
		
		
		
		
		return this.point;
	}
}
