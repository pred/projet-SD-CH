package sd;

public class Directeur {

	private String nom;
	private String prenom;
	private int point;

	public Directeur(String prenom,String nom){
		this.prenom=prenom;
		this.nom=nom;
		this.point=0;
	}
	
	public String getPrenom(){
		return this.prenom;
	}
	public String getNom(){
		return this.nom;
	}
	
	public void augmentePoint(){
		this.point++;
	}
}
