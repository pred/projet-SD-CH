package sd;

import java.util.List;
import java.util.ArrayList;

public class Film {
	String Titre;
	int Année;
	String Type;
	String Synopsis;
	String Directeur;
	List<Acteur> Acteurs = new ArrayList<String>();
	List<Genre> Genre = new ArrayList<String>();
	String Duree;
	
	public Film() {
		Titre = "inconnu";
		Année = 0;
		Type = "inconnu";
		Synopsis = "inconnu";
		Directeur = "inconnu";
		Acteurs=null;
		Genre=null;
		Duree="inconnue";
	}
}
